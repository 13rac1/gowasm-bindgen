{
  "type": "module",
  "scripts": {
    "lint": "eslint 'example/**/*.ts' 'test/**/*.ts'",
    "lint:fix": "eslint --fix 'example/**/*.ts' 'test/**/*.ts'",
    "format": "prettier --write 'example/**/*.ts' 'test/**/*.ts'",
    "format:check": "prettier --check 'example/**/*.ts' 'test/**/*.ts'",
    "typecheck": "tsc --noEmit -p example/tsconfig.json && tsc --noEmit -p example/web/tsconfig.json && tsc --noEmit -p test/e2e/tsconfig.json"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "c8": "^10.1.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.8.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0"
  }
}
